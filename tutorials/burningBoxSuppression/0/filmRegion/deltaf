FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/filmRegion";
    object      deltaf;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    fuel_top
    {
        type            zeroGradient;
    }
    region0_to_filmRegion_fuel
    {
        type            zeroGradient;
    }
}
