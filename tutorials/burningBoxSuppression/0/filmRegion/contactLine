FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/filmRegion";
    object      contactLine;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    fuel_top
    {
        type            zeroGradient;
    }

    region0_to_filmRegion_fuel
    {
        type            zeroGradient;
    }
}
